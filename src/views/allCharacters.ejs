<%- include('./header.ejs') %>
<ul>
  <% characters.sort((a, b) => a.name.toUpperCase() < b.name.toUpperCase() ? -1 : a.name.toUpperCase() > b.name.toUpperCase() ? 1 : 0).forEach(character => { %>
    <li class=<%= character.rarity === "5" ? "five-star" : "four-star" %>>
      <a href=<%= character.url %>>
        <h3>
          <%= character.name %>
        </h3>
        <img 
          src=<%= character.imgPath %>
          alt=<%= character.name %>
        >
      </a>
    </li>
  <% }) %>
</ul>
<%- include('./footer.ejs') %>
